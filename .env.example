# FBA-Bench environment example

# LLM providers
OPENROUTER_API_KEY=
OPENAI_API_KEY=

# Backend services
FBA_BENCH_REDIS_URL=redis://localhost:6379/0
FBA_BENCH_DB_URL=sqlite:///./fba_bench.db

# Observability
ENABLE_OBSERVABILITY=true
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318

# Runtime profile
PROFILE=production
LOG_LEVEL=INFO

# Optional health checks toggles
CHECK_REDIS=false
CHECK_DB=false

# API server network (used by local dev helpers)
API_HOST=0.0.0.0
API_PORT=8000